/* pages/myData/myData.wxss */
.myDataWrapper{
  padding: 20rpx;
  font-size: 40rpx;
}
.myDataTitle{
  background: #333;
  color: #fff;
  text-align: center;
}
.myDatainput{
  border: 1px solid #333;
  border-bottom-right-radius: 8rpx;
  border-bottom-left-radius: 8rpx;
  text-align: center;
  padding: 20rpx 0;
  margin-bottom: 20rpx;
  color: #333;
  min-height: 110rpx
}
.userinfo-avatar{
  width:128rpx;
  height:128rpx;
  margin:20rpx;
  border-radius:50%;
}
.userinfo{
  display:flex;
  flex-direction:column;
  align-items:center;
  margin-bottom: 20rpx;
}
.userinfo-nickname{
  color: #aaa;
}
.flexInfo{
  display: flex;
}
.locaInfo{
  flex: 1;
}